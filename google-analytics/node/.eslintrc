{
  "extends": "airbnb-base",
  "env": {
    "es6": true
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "semi": [
      "error",
      "always"
    ],
    "arrow-body-style": [
      2,
      "as-needed"
    ],
    "max-len": [
      "error",
      150
    ],
    "import/extensions": 0
  }
}
